{
  "version": 2,
  "name": "pandora-cloud",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "main.py"
    }
  ],
  "env": {
    "PANDORA_SERVER_LISTEN": "false"
  },
  "frameworks": {
    "node": {
      "version": "14.x"
    }
  }
}
